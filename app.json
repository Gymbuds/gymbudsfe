{
  "expo": {
    "name": "gymbudfrontend",
    "slug": "gymbudfrontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "bundleIdentifier": "com.yourcompany.gymbuds",
      "supportsTablet": true,
      "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to change your profile picture."
    },
    "android": {
      "package": "com.yourcompany.gymbuds",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCv0H_JQ1RwiISCjUMq48rmnBs4FMmUG3A"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
